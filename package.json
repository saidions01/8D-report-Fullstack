{
  "name": "8d-report-deloitte",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
  
    "install-all": "npm-run-all --parallel install-backend install-frontend",
    "install-backend": "cd Backend && npm install",
    "install-frontend": "cd deloitte-8d-solution && npm install",
    "dev": "concurrently \"npm run dev-backend\" \"npm run dev-frontend\"",
    "dev-backend": "cd Backend && npm run dev",
    "dev-frontend": "cd deloitte-8d-solution && npm run dev",
    "build": "npm run build-frontend && npm run build-backend",
  "build-frontend": "cd deloitte-8d-solution && npm install && npm run build && cp -r build ../backend/public",
  "build-backend": "cd backend && npm install",
  "start": "cd backend && npm start"
  
  
  },
